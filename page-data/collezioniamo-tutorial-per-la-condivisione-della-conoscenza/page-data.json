e.setAttribute(\"srcset\",e.dataset.srcset),e.removeAttribute(\"data-srcset\"));const t=e.parentNode.querySelectorAll(\"source[data-srcset]\");for(let e of t)e.setAttribute(\"srcset\",e.dataset.srcset),e.removeAttribute(\"data-srcset\");e.complete&&(e.style.opacity=1)}}</script></div><script type=\"application/json\" data-wp-inline-image-hydration=\"4\">{\"image\":{\"layout\":\"constrained\",\"backgroundColor\":\"#f8f8f8\",\"images\":{\"fallback\":{\"src\":\"/opendatasicilia.it/static/665073c29863c907fb6741ed5790a694/01065/accussi.png\",\"srcSet\":\"/opendatasicilia.it/static/665073c29863c907fb6741ed5790a694/e8cec/accussi.png 200w,\\n/opendatasicilia.it/static/665073c29863c907fb6741ed5790a694/dae54/accussi.png 400w,\\n/opendatasicilia.it/static/665073c29863c907fb6741ed5790a694/01065/accussi.png 800w\",\"sizes\":\"(min-width: 800px) 800px, 100vw\"},\"sources\":[{\"srcSet\":\"/opendatasicilia.it/static/665073c29863c907fb6741ed5790a694/f792c/accussi.webp 200w,\\n/opendatasicilia.it/static/665073c29863c907fb6741ed5790a694/807a4/accussi.webp 400w,\\n/opendatasicilia.it/static/665073c29863c907fb6741ed5790a694/19e6b/accussi.webp 800w\",\"type\":\"image/webp\",\"sizes\":\"(min-width: 800px) 800px, 100vw\"}]},\"width\":800,\"height\":595},\"alt\":\"\",\"className\":\"size-full wp-image-1675 alignnone inline-gatsby-image-wrapper\",\"data-wp-inline-image\":\"4\"}</script></a></p>\n<p class=\"graf graf--p\">Oggi c’è la disponibilità pubblica di questo catalogo interrogabile su parole chiave, su autore e su tag. Abbiamo previsto una pagina “<a class=\"markup--anchor markup--p-anchor\" href=\"http://accussi.opendatasicilia.it/proponi.html\" target=\"_blank\" rel=\"noopener noreferrer\"><strong class=\"markup--strong markup--p-strong\">proponi</strong></a>” al fine di raccogliere facilmente quanto più tutorial possibili. Chiunque abbia costruito un tutorial sull&#8217;uso dei dati, su come si fa una mappa, un infografica, una trasformazione di file in formati diversi (da pdf a csv per es.), e su cultura digitale in generale, può censirlo in questa pagina, arricchendo il catalogo, che diventa, così, un patrimonio di conoscenza comune.</p>\n<p class=\"graf graf--p\"><a href=\"https://i0.wp.com/opendatasicilia.it/wp-content/uploads/2017/05/db_accussì.png?ssl=1\"><div data-gatsby-image-wrapper=\"\" style=\"position:relative;overflow:hidden;display:inline-block;vertical-align:top\" class=\"gatsby-image-wrapper gatsby-image-wrapper-constrained size-full wp-image-1676 alignnone inline-gatsby-image-wrapper\" data-reactroot=\"\"><div style=\"max-width:800px;display:block\"><img alt=\"\" role=\"presentation\" aria-hidden=\"true\" src=\"data:image/svg+xml;charset=utf-8,%3Csvg height=&#x27;427&#x27; width=&#x27;800&#x27; xmlns=&#x27;http://www.w3.org/2000/svg&#x27; version=&#x27;1.1&#x27;%3E%3C/svg%3E\" style=\"max-width:100%;display:block;position:static\"/></div><div aria-hidden=\"true\" data-placeholder-image=\"\" style=\"height:100%;left:0;position:absolute;top:0;width:100%\"></div><picture><source type=\"image/webp\" data-srcset=\"/opendatasicilia.it/static/eee9f09b8a83b6e3889912ec539e61a7/e17ef/db_accussi.webp 200w,/opendatasicilia.it/static/eee9f09b8a83b6e3889912ec539e61a7/5d2d8/db_accussi.webp 400w,/opendatasicilia.it/static/eee9f09b8a83b6e3889912ec539e61a7/b80eb/db_accussi.webp 800w\" sizes=\"(min-width: 800px) 800px, 100vw\"/><img data-gatsby-image-ssr=\"\" data-wp-inline-image=\"5\" data-main-image=\"\" style=\"height:100%;left:0;position:absolute;top:0;transform:translateZ(0);transition:opacity 250ms linear;width:100%;will-change:opacity;opacity:0\" sizes=\"(min-width: 800px) 800px, 100vw\" decoding=\"async\" loading=\"lazy\" data-src=\"/opendatasicilia.it/static/eee9f09b8a83b6e3889912ec539e61a7/88820/db_accussi.png\" data-srcset=\"/opendatasicilia.it/static/eee9f09b8a83b6e3889912ec539e61a7/07548/db_accussi.png 200w,/opendatasicilia.it/static/eee9f09b8a83b6e3889912ec539e61a7/76300/db_accussi.png 400w,/opendatasicilia.it/static/eee9f09b8a83b6